<mvc:View
	controllerName="zappfreestylelpb.parceiroslpb.controller.CriarParceiro"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	displayBlock="true"
	height="100%"
    xmlns:uxap="sap.uxap"
    xmlns:form="sap.ui.layout.form"
	busyIndicatorDelay="0"

>
<Page id="page" title="{i18n>title}">
   <uxap:ObjectPageLayout
       alwaysShowContentHeader="false"
       backgroundDesignAnchorBar=""
       enableLazyLoading="false"
       flexEnabled="false"
       headerContentPinnable="true"
       height="100%"
       width="50%"
       isChildPage="false"
       preserveHeaderStateOnScroll="false"
       sectionTitleLevel="Auto"
       showAnchorBar="true"
       showAnchorBarPopover="true"
       showEditHeaderButton="false"
       showFooter="true"
       showHeaderContent="true"
       showOnlyHighImportance="false"
       showTitleInHeaderContent="false"
       subSectionLayout="TitleOnTop"
       toggleHeaderOnTitleClick="true"
       upperCaseAnchorBar="true"
       useIconTabBar="false"
       useTwoColumnsForLargeScreen="false"
   >
      <uxap:headerTitle>
        <uxap:ObjectPageHeader objectTitle="Criar Parceiro" />
      </uxap:headerTitle>
      <uxap:headerContent>
        <ObjectAttribute  title="Digite as informações para criar um novo parceiro" textDirection="Inherit"  text="" />
      </uxap:headerContent>
      <uxap:sections>
        <uxap:ObjectPageSection  showTitle="true"  titleUppercase="false" title="Dados Parceiro" visible="true">
            <uxap:subSections>
                <uxap:ObjectPageSubSection mode="Expanded" showTitle="true" titleUppercase="false" visible="true">
                    <uxap:blocks>
                        <form:SimpleForm
                            id="DadosParceiro"
                            title=""
                            adjustLabelSpan="true"
                            backgroundDesign="Solid"
                            breakpointL="1024"
                            breakpointM="600"
                            breakpointXL="1440"
                            columnsL="2"
                            columnsM="1"
                            columnsXL="-1"
                            editable="true"
                            emptySpanL="0"
                            emptySpanM="0"
                            emptySpanS="0"
                            emptySpanXL="-1"
                            labelMinWidth="192"
                            labelSpanL="2"
                            labelSpanM="2"
                            labelSpanS="12"
                            labelSpanXL="-1"
                            layout="ResponsiveGridLayout"
                            maxContainerCols="2"
                            minWidth="-1"
                            singleContainerFullSize="true"
                            width=""
                        >
                            <form:content>
                                <Label text="Tipo" />
                                <Input value="{Tipo}" editable="true" with="50%"/> 
                                <Label text="Nome" />
                                
                                <!--<ComboBox editable="true"  enabled="true" items="{path:'/TipoParceirosSet', templateShareable:true}" maxWidth="100%"  >
                                </ComboBox>-->
                            </form:content>
                        </form:SimpleForm>
                    </uxap:blocks>
                </uxap:ObjectPageSubSection>
            </uxap:subSections>
        </uxap:ObjectPageSection>
      </uxap:sections>

   </uxap:ObjectPageLayout>
</Page>
</mvc:View>